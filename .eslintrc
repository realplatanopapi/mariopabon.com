{
  "ecmaFeatures": {
    "modules": false
  },
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "rules": {
    "brace-style": 2,
    "comma-dangle": 2,
    "curly": 2,
    "dot-notation": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-undef": 2,
    "no-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-catch-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "semi": 2,
    "use-isnan": 2,
    "valid-typeof": 2
  }
}
